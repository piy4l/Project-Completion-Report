<script>
    function saveProject(){
        
        var payload = {
            Name: $("#secA_ProjectName").val(),
            AdministrativeMinistryDivision: $("#secA_MinistryDivision"). val(),
			ExecutingAgency: $("#secA_Agency").val(),
			PlanningCommissionSectorDivision: $("#secA_PlanningSector").val(),           
        };

		// console.log(payload);
            
        $.ajax({
            type: "POST",
            url: "/Projects/SaveProject",
            data: JSON.stringify(payload),
            contentType: "application/json; charset=utf-8", // Inform the server of the JSON format
            dataType: "json", // Expected data type from the server
            success: function (response) {
                console.log("Success:", response.message);
				alert("Project saved successfully");
                // refresh the page
				location.reload();
            },
            error: function (error) {
                console.error("Error:", error.responseText);
				alert("Error: " + error.responseText);
            }
        });
    }
</script>
